{
    "name": "activecollab/authentication",
    "type": "library",
    "description": "Authenticate user",
    "license": "MIT",
    "keywords": [
        "authentication",
        "activecollab"
    ],
    "homepage": "https://labs.activecollab.com",
    "authors": [
        {
            "name": "Ilija Studen",
            "email": "ilija.studen@activecollab.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-mbstring": "*",
        "activecollab/cookies": "^2.0",
        "activecollab/user": "^4.0",
        "google/apiclient": "^2.1",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "lightsaml/lightsaml": "^1.1",
        "phpunit/phpunit": "^7.0",
        "satooshi/php-coveralls": "^1.0",
        "zendframework/zend-diactoros": "^2.2",
        "pimple/pimple": "^3.2"
    },
    "suggest": {
        "lightsaml/lightsaml": "If you want to use SAML authentication you should manually install this package."
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "ActiveCollab\\Authentication\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActiveCollab\\Authentication\\Test\\": "test/src"
        }
    }
}
