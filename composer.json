{
    "name": "activecollab/authentication",
    "type": "library",
    "description": "Authenticate user",
    "license": "MIT",
    "prefer-stable": true,
    "keywords": [
        "authentication",
        "activecollab"
    ],
    "homepage": "https://labs.activecollab.com",
    "authors": [
        {
            "name": "Ilija Studen",
            "email": "ilija.studen@activecollab.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "activecollab/user": "~1.0",
        "activecollab/cookies": "~0.1",
        "guzzlehttp/psr7": "~1.2"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.0",
        "phpunit/phpunit": "~5",
        "slim/slim": "3.0.0-RC2"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "ActiveCollab\\Authentication\\": "src"
        }
